{
  "private": true,
  "name": "borksy",
  "version": "4.6.6",
  "description": "Borksy Game Hacker: A tool for extending Bitsy games",
  "scripts": {
    "postversion": "npm run build",
    "build": "rm -r -f ./docs && cp -r ./src ./docs && rm ./docs/about/*.md && rm -r ./docs/test",
    "test": "jest --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ayolland/borksy.git"
  },
  "author": "AYolland",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Ayolland/borksy/issues"
  },
  "homepage": "https://ayolland.itch.io/borksy",
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/test/setupTests.js"
    ]
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs/**/*",
            "CHANGELOG.md",
            "package.json",
            "package-lock.json"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "jest": "^27.2.5",
    "jest-image-snapshot": "^4.5.1",
    "puppeteer": "^10.4.0"
  }
}
